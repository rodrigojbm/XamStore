<!DOCTYPE html>
<html>
<head>
    <title>Infinity Games</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="google-signin-scope" content="profile email">
    <meta name="google-signin-client_id" content="1035537579914-su809tar1augpqpj0hcrl2tmgdc080r0.apps.googleusercontent.com">
    <link href="../../Content/style.css" rel="stylesheet" type="text/css" media="all"/>
    <!--- start-mmmenu-script---->
    <script src="../../Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
    <link type="text/css" href="~/Content/bootstrap.css" rel="stylesheet"/>
    <link type="text/css" rel="stylesheet" href="../../Content/jquery.mmenu.all.css"/>
    <script type="text/javascript" src="../../Scripts/jquery.mmenu.js"></script>
    <script type="text/javascript">
        //	The menu on the left
        $(document).ready(function() {
            $(function() {
                $('nav#menu-left').mmenu();
            });
        });
    </script>
    <!-- start slider -->

    <link href="../../Content/slider.css" rel="stylesheet" type="text/css" media="all"/>
    <script type="text/javascript" src="../../Scripts/jquery.eislideshow.js"></script>
    <script type="text/javascript" src="../../Scripts/easing.js"></script>
    <script type="text/javascript">
        $(document).ready(function() {
            $(function() {
                $('#ei-slider').eislideshow({
                    animation: 'center',
                    autoplay: true,
                    slideshow_interval: 3000,
                    titlesFactor: 0
                });

                var url = window.location.origin;
                (function checkStatusPagseguro() {

                    setTimeout(function() {
                        $.ajax({
                            type: "POST",
                            url: url + "/carrinho/checkStatusPagseguro",
                            complete: checkStatusPagseguro,
                            timeout: 5000
                        });
                    }, 60000);
                })();
            });
        });
    </script>

    <!-- start top_js_button -->
    <script type="text/javascript" src="../../Scripts/move-top.js"></script>
    <script type="text/javascript">
        jQuery(document).ready(function($) {
            $(".scroll").click(function(event) {
                event.preventDefault();
                $('html,body').animate({ scrollTop: $(this.hash).offset().top }, 1200);
            });
        });
    </script>
</head>
<body>
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/facebook")
    @Scripts.Render("~/bundles/custom")


    @Html.Partial("Header")
    @Html.Partial("Menu")
    @RenderBody()
    @Html.Partial("Footer")

</body>
</html>